{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "app",
    "components",
    "types",
    "services",
    "context",
    "node_modules/@types",
    ".expo",
    "node_modules",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
